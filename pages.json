{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/errand/errand",
			"style": {
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/life/life",
			"style": {
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/order/order",
			"style": {
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/order/childCmps/detail/detail",
			"style": {
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/user/user",
			"style": {
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/errand/childCmps/errandOrder",
			"style": {
				"enablePullDownRefresh": false
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "登录界面",
				"enablePullDownRefresh": false
			}
		}
	],
	//分包以pages内部目录为划分
	"subPackages": [
		{
			"root": "pages/user/childCmps",
			"pages": [
				{
					"path": "address/address",
					"style": {
						"enablePullDownRefresh": false
					}
				},
				{
					"path": "info/info",
					"style": {
						"enablePullDownRefresh": false
					}
				}
			]
		},
		{
			"root": "pages/index/childCmps",
			"pages": [
				{
					"path": "news/newsDetail",
					"style": {
						"enablePullDownRefresh": false
					}
				},
				{
					"path": "classroom/classroom",
					"style": {
						"enablePullDownRefresh": false
					}
				}
			]
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "触手校园",
		"navigationBarBackgroundColor": "#f4f7fc",
		"backgroundColor": "#f4f7fc",
		"navigationStyle": "custom", //导航栏样式设置成自定义导航栏
		"enablePullDownRefresh": true
	},
	"tabBar": {
		"color": "#666",
		"selectedColor": "#A493FF",
		"list": [
			{
				"text": "应用",
				"pagePath": "pages/index/index",
				"iconPath": "static/image/tabbar/index.png",
				"selectedIconPath": "static/image/tabbar/index-active.png"
			},
			{
				"text": "跑腿",
				"pagePath": "pages/errand/errand",
				"iconPath": "static/image/tabbar/order.png",
				"selectedIconPath": "static/image/tabbar/order-active.png"
			},
			{
				"text": "订单",
				"pagePath": "pages/order/order",
				"iconPath": "static/image/tabbar/sorder.png",
				"selectedIconPath": "static/image/tabbar/sorder-active.png"
			},
			{
				"text": "Life",
				"pagePath": "pages/life/life",
				"iconPath": "static/image/tabbar/life.png",
				"selectedIconPath": "static/image/tabbar/life-active.png"
			},
			{
				"text": "用户",
				"pagePath": "pages/user/user",
				"iconPath": "static/image/tabbar/user.png",
				"selectedIconPath": "static/image/tabbar/user-active.png"
			}
		]
	},
	"uniIdRouter": {}
}
